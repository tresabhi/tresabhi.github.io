---
import type { Tokens } from "marked";
import type { TokenRendererProps } from "./TokenRenderer.astro";

export interface MacroToken extends Tokens.Generic {
  type: "latex";
  name: string;
  parameters: string;
}

type Props = TokenRendererProps<MacroToken>;

const { token } = Astro.props;
---

<b>@{token.name}</b>
<pre>{token.parameters}</pre>
